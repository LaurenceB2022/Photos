<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="photos.SlideshowController"
            prefHeight="400.0" prefWidth="400.0">
    <VBox spacing="25">
    <Button AnchorPane.leftAnchor="10" AnchorPane.topAnchor="10" text="Go Back" onAction="#goBack"/>
    <HBox spacing="15">
        <Button AnchorPane.leftAnchor="10" AnchorPane.topAnchor="100" text="&lt;-" onAction="#prevPicture"/>
        <ImageView fx:id="picture"/>
        <Button AnchorPane.rightAnchor="10" AnchorPane.topAnchor="100" text="-&gt;" onAction="#nextPicture"/>

    </HBox>
    </VBox>

</AnchorPane>
